#include <stdio.h>

int main()
{
    for(int i=0;i<5;i++)
    {
        for(int j=5;j>i;j--)
        {
            printf("*");
        }
        for(int k=0;k<i;k++)
        {
            printf(" ");
        }
        for(int l=0;l<i;l++)
        {
            printf(" ");
        }
        for(int l=5;l>i;l--)
        {
            printf("*");
        }
        printf("\n");
    }
    for(int m=1;m<=5;m++)
    {
            for(int n=1;n<=m;n++)
        {
            printf("*");
        }
        for(int o=5;o>m;o--)
        {
            printf(" ");
        }
        for(int p=5;p>m;p--)
        {
            printf(" ");
        }
        for(int q=1;q<=m;q++)
        {
            printf("*");
        }
            printf("\n");
    }
        

    return 0;
}
